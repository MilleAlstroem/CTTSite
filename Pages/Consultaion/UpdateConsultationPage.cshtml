@page
@model CTTSite.Pages.Consultation.UpdateConsultationPageModel
@{
}


<form method="post">
    <div class="form-group">
        <label asp-for="@Model.Consultation.Date" class="control-lable"></label>
        <input asp-for="@Model.Consultation.Date" class="form-control" />
        <span asp-validation-for="@Model.Consultation.Date" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="@Model.Consultation.StartTime" class="control-lable">Start Time</label>
        <input asp-for="@Model.Consultation.StartTime" class="form-control" />
        <span asp-validation-for="@Model.Consultation.StartTime" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="@Model.Consultation.EndTime" class="control-lable">End Time</label>
        <input asp-for="@Model.Consultation.EndTime" class="form-control" />
        <span asp-validation-for="@Model.Consultation.EndTime" class="text-danger"></span>
    </div>

    <div class="form-group">
        <input type="submit" value="Update" class="btn btn-primary" />
    </div>
</form>